   \ifpdf
      \immediate\write\@auxout{\string
      \newlabel{LastAppendix}{{}{\theappendix}{}{}{}}}%
   \else
      \immediate\write\@auxout{\string
      \newlabel{LastAppendix}{{}{\theappendix}}}%
   \fi
   \addtocounter{page}{-1}
   \ifpdf
      \immediate\write\@auxout{\string
      \newlabel{count\theappendix}{{}{\thepage}{}{}{}}}%
   \else
      \immediate\write\@auxout{\string
      \newlabel{count\theappendix}{{}{\thepage}}}%
   \fi
   \setcounter{lastpage}{\thepage}
   \ifpdf
      \immediate\write\@auxout{\string
      \newlabel{LastPage}{{}{\thelastpage}{}{}{}}}%
   \else
      \immediate\write\@auxout{\string
      \newlabel{LastPage}{{}{\thelastpage}}}%
   \fi
   }
